package homework3;

public class test_p9 {
	public static void main(String[] args) {
		
	car_class9 car1;
	car1=new car_class9();
	
	car1.num=1234;
	car1.gas=20.5;	
	}
	

}

class car_class9{
	
	int num;
	double gas;
	void show()
	{
		System.out.println("車號是"+this.num);
		System.out.println("汽油量是"+this.gas);
	}
}
